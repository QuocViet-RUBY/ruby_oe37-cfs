<tr>
  <td scope="row"><%= user.full_name %></td>
  <td scope="row"><%= user.email %></td>
  <td scope="row"><%= user.activated = t("manger_user.activate")%></td>
  <td scope="row"><%= l(user.activated_at, format: :long) %></td>
  <td scope="row"><%= link_to t("manger_user.delete"), user, method: :delete %></td>
  <td scope="row"><%= link_to t("manger_user.show"), admin_user_path(current_user) %></td>
</tr>
